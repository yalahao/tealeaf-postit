<%= render 'shared/error_msg', obj: @post %>

<div class="well">
  <%= form_for @post do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :url %>
    <%= f.text_field :url %>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <br />
    <%= f.submit %>
  <% end %>
</div>