<div class='well'>
  <%= form_for @user do |f| %>
  <%= render 'shared/error_msg', obj: @user %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br />
    <%= f.submit @user.new_record? ? "Register" : "Update", class: 'btn btn-success' %>
  <% end %>
</div>
